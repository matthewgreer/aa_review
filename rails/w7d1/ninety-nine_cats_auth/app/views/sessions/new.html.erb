<h1>Login For Kittehs</h1>

<%= render 'shared/errors'  %>

<form action="/session" method="POST">
  <input 
    type="hidden" 
    name="authenticity_token" 
    value="<%= form_authenticity_token %>"
  >
  <p>
  <label for="user_at_user_name">Yoozer Naem</label><br>
  <input
    type="text"
    name="user[user_name]"
    id="user_at_user_name"
  >
  </p>
  <p>
    <label for="user_at_user_password">Paws-Word</label><br>
    <input
      type="password"
      name="user[password]"
      id="user_at_user_password"
    >
  </p>

  
  <p><input class="button" type="submit" value="Submit, Yoozer!"></p>
</form>
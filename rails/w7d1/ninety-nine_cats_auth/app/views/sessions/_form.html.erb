<form action="/session" method="POST">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>
    <label for="user_at_user_name">Username: </label>
    <input type="text" name="user[user_name]" id="user_at_user_name">
  </p>
  <p>
    <label for="user_at_password">Password</label>
    <input type="text" name="user[password]" id="user_at_password">
  </p>
  <p>
    <input type="submit" value="Log In">
</form>